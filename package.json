{
  "name": "graphql-apollo-server-express-typescript-seed",
  "version": "1.0.0",
  "description": "GraphQL Apollo-Server-Express Typescript seed",
  "main": "server.js",
  "scripts": {
    "build": "tsc",
    "dev": "concurrently \"nodemon src/server.ts\" \"npm run generate\"",
    "generate": "graphql-codegen --config codegen.yml",
    "lint": "npx eslint ./src/**",
    "prepare": "npm run generate",
    "start": "npm run build && node dist/server.js",
    "start:prod": "node dist/server.js"
  },
  "keywords": [
    "apollo-server-express",
    "graphql",
    "typescript",
    "seed"
  ],
  "author": "Tal Shperling",
  "license": "ISC",
  "devDependencies": {
    "@graphql-codegen/cli": "6.1.1",
    "@graphql-codegen/introspection": "5.0.0",
    "@graphql-codegen/typescript": "5.0.7",
    "@graphql-codegen/typescript-resolvers": "5.1.5",
    "@types/compression": "1.8.1",
    "@types/cors": "2.8.19",
    "@types/express": "4.17.25",
    "@types/graphql": "14.5.0",
    "@types/lodash": "4.17.20",
    "@types/node": "14.18.63",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "concurrently": "5.3.0",
    "eslint": "7.32.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-n": "14.0.0",
    "eslint-plugin-promise": "4.3.1",
    "jsdoc": "3.6.11",
    "nodemon": "2.0.22",
    "prettier": "2.8.8",
    "rimraf": "3.0.2",
    "ts-node": "9.1.1",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "apollo-server-express": "^2.21.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "graphql": "^15.3.0",
    "graphql-subscriptions": "^1.2.0",
    "lodash": "^4.17.20",
    "mongodb": "^3.6.4"
  }
}
